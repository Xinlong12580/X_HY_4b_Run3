{
    "NAME": "XYH_test",
    "GLOBAL": {
        "FILE": "Templates_all.root",
        "HIST": "$process_$region_nominal",
        "HIST_UP": "$process_$region_$syst_up",
        "HIST_DOWN": "$process_$region_$syst_down",
        "PATH_LOCAL": ".",
        "SIGNAME": [
            "SignalMC_XHY4b"
        ], 
        "COMMENT": "NOTE: I will be using the local rootfiles. I will also include the year in the process name, so that other years can be added easily later. The python script has a findreplace dictionary which will insert the XMASS and YMASS values in signame above to specify the correct signal. This is only relevant if you hvae multiple different signals in the SIGNAME list above, in which case multiple signals are added to the 2DA ledger and you can select which one to create a Combine card for."
    },
    "REGIONS": {
        "VB1": {
            "PROCESSES": [
                "data_obs",
                "2022_SIGNAME",
                "2022_MC_TTBarJets",
                "2022_MC_WZJets"
            ],
            "BINNING": "default"
        },
        "VS2": {
            "PROCESSES": [
                "data_obs",
                "2022_SIGNAME",
                "2022_MC_TTBarJets",
                "2022_MC_WZJets"
            ],
            "BINNING": "default"
        }
    },
    "PROCESSES": {
        "data_obs": {
            "SYSTEMATICS": [],
            "SCALE": 1.0,
            "COLOR": "black",
            "TYPE": "DATA",
            "ALIAS": "2022_JetMET",
            "TITLE": "Data ",
            "LOC": "PATH_LOCAL/FILE:HIST"
        },
        "2022_SIGNAME": {
            "SYSTEMATICS": [
                "lumi_13p6TeV",
                "JES_",
                "JER_",
                "PileUp_Corr"
            ],
            "SCALE": 1.0,
            "COLOR": "black",
            "TYPE": "SIGNAL",
            "TITLE": "NMSSM_XYH",
            "LOC": "PATH_LOCAL/FILE:HIST"
        },
        "2022_MC_TTBarJets": {
            "SYSTEMATICS": [
                "lumi_13p6TeV",
                "JES_",
                "JER_",
                "PileUp_Corr"
            ],
            "SCALE": 1.0,
            "COLOR": "red",
            "TYPE": "BKG",
            "TITLE": "t#bar{t}",
            "LOC": "PATH_LOCAL/FILE:HIST"
        },
        "2022_MC_WZJets": {
            "SYSTEMATICS": [
                "lumi_13p6TeV",
                "JES_",
                "JER_",
                "PileUp_Corr"
            ],
            "SCALE": 1.0,
            "COLOR": "magenta",
            "TYPE": "BKG",
            "TITLE": "V+jets",
            "LOC": "PATH_LOCAL/FILE:HIST"
        }
    },



    "SYSTEMATICS": {
        "lumi_13p6TeV": {
            "VAL": 1.05,
            "CODE": 0
        },
        "JES_": {
            "UP": "PATH_LOCAL/FILE:HIST_UP",
            "DOWN": "PATH_LOCAL/FILE:HIST_DOWN",
            "SIGMA": 1.0
        },
        "JER_": {
            "UP": "PATH_LOCAL/FILE:HIST_UP",
            "DOWN": "PATH_LOCAL/FILE:HIST_DOWN",
            "SIGMA": 1.0
        },
        "PileUp_Corr": {
            "UP": "PATH_LOCAL/FILE:HIST_UP",
            "DOWN": "PATH_LOCAL/FILE:HIST_DOWN",
            "SIGMA": 1.0
        }
    },
    "BINNING": {
        "default": {
            "X": {
                "NAME": "M_JY",
                "TITLE": "M_{J}^{Y}",
                "BINS": [60,100,140,200,300, 500],
                "SIGSTART": 140,
                "SIGEND": 200
            },
            "Y": {
                "NAME": "M_JJ",
                "TITLE": "M_{JJ}",
                "BINS": [960,1000,1040,1080,1120,1160,1200,1240,1280,1320,1360,1400,1440,1480,1520,4000]
            }
        }
    },
    "OPTIONS": {
        "blindedPlots": [],
        "blindedFit": [],
        "plotPrefitSigInFitB": false,
        "plotUncerts": true,
        "plotEvtsPerUnit": true,
        "plotTitles": false,
        "plotTemplateComparisons": true,
        "year": 2
    }
}
